            <td valign="top" colspan="1" align="left">
            <strong>008/31</strong></td>

            <td valign="top" colspan="8" align="left">1</td>
          </tr>

          <tr>
            <td valign="top" colspan="1" align="left">500</td>

            <td valign="top" colspan="9" align="left">##$aIndex
            to points of interest on verso.</td>
          </tr>
        </table>
      </div>

      <div class="example">
        <table width="100%">
          <tr>
            <td valign="top" width="5%" align="left"></td>

            <td valign="top" width="10%" align="left"></td>

            <td valign="top" width="10%" align="left"></td>

            <td valign="top" width="10%" align="left"></td>

